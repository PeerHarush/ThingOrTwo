
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Song {
  id        Int      @id @default(autoincrement())
  title     String
  band      String
  year      Int?
  createdAt DateTime @default(now())
}


// Defines the database schema for the project
// Contains the Song model with title, band, year and creation date